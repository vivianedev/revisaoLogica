using System;

class MainClass {
  public static void Main (string[] args) {
    int valor, dentro = 0, fora = 0;
    int x = int.Parse(Console.ReadLine());
    for (int y = 1; y <= x; y++) {
      valor = int.Parse(Console.ReadLine());
      if (valor >= 10 && valor <= 20) {
        dentro++;
      }
      else fora++;
    }
    Console.WriteLine($"{dentro} in\n{fora} out");
  }
}
